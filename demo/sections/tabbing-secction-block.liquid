<div class="page-width">
  <ul class="title-tabbing">

    <li><a class="tabbing-title-url nav-active" href="#tab-{{section.settings.cus_title1}}">{{section.settings.cus_title1}}</a></li>
    <li><a class="tabbing-title-url" href="#tab-{{section.settings.cus_title2}}">{{section.settings.cus_title2}}</a></li>
    <li><a class="tabbing-title-url" href="#tab-{{section.settings.cus_title3}}">{{section.settings.cus_title3}}</a></li>
    <li><a class="tabbing-title-url" href="#tab-{{section.settings.cus_title4}}">{{section.settings.cus_title4}}</a></li>
