{% schema %}
{
   "name":"tabbing secction block",
   "settings":[
      {
        "type":"text",
        "id":"cus_title1",
        "label":"Title"
      },
	  {
        "type":"text",
        "id":"cus_title2",
        "label":"Title"
      },
	  {
        "type":"text",
        "id":"cus_title3",
        "label":"Title"
      },
	  {
        "type":"text",
