    <li><a class="tabbing-title-url" href="#tab-">{{section.settings.cus_title6}}</a></li>

  </ul>

  <div class="cus-block-product-images">

    <div class="cus-block-img-filter active" id="tab-{{section.settings.cus_title1}}">
      <div class="cus-productimg-set">
        {% assign count = 0 %}
        {% for block in section.blocks %}
        {% assign num_count = count | plus: 4 %}
        {% if section.settings.cus_title1 == block.settings.block_title %}
        <div class="filter-image-with-text " data-counter="{{num_count}}">
          <img src="{{block.settings.logo | img_url: 'master' }}" alt="logo" />
          <div class="cus-title-with-btn">
            <div class="cus-title-with-desc">
              <h5>{{block.settings.block_title}}</h5>
              <p>{{block.settings.block_desc}}</p>
            </div>
            <button class="cus-readmore-btn btn">Read More</button>
          </div>
        </div>
        {% endif %}
        {% endfor %}
      </div>
      <div class="showmore-btn-t"> 
      	<button class="cus-showmore-btn btn" disabled="disabled" data-counter data-count-total>Show More</button>
      </div>
    </div>

